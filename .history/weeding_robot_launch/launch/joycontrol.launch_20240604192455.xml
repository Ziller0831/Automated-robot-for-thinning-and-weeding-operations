<?xml version="1.0"?>
<launch>
    <node pkg="joy" exec="joy_node">
        <param name="device_name" value
    </node>
    <node pkg="gamepad_controller" exec="joy_controller" />
    <node pkg="micro_ros_agent" exec="micro_ros_agent" args="serial --dev /dev/ttyUSB0"
        output="screen" />
</launch>   